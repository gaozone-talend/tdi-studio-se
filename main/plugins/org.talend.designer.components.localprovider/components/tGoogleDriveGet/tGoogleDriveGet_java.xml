<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20140417A"
    STARTABLE="true"
    
    SCHEMA_AUTO_PROPAGATE="false"
    DATA_AUTO_PROPAGATE="false"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Cloud/Google Drive</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1" NOT_SHOW_IF="STORE_TO_LOCAL == 'true' "/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" MAX_OUTPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" MAX_OUTPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK" MAX_INPUT="1" MAX_OUTPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_ERROR" MAX_INPUT="1" MAX_OUTPUT="1" />
    <CONNECTOR CTYPE="RUN_IF" MAX_INPUT="0" MAX_OUTPUT="0"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER NAME="CONNECTION" FIELD="COMPONENT_LIST"  FILTER="tGoogleDriveConnection" NUM_ROW="10"  DYNAMIC_SETTINGS="true"/>
    
 	<PARAMETER NAME="BY_PATH" FIELD="RADIO" NUM_ROW="13">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>
 	<PARAMETER NAME="BY_ID" FIELD="RADIO" NUM_ROW="13">
		<DEFAULT>true</DEFAULT>
	</PARAMETER>
		
    <PARAMETER NAME="PATH"  FIELD="TEXT"  NUM_ROW="54" REQUIRED="true" SHOW_IF="BY_PATH == 'true'">
      <DEFAULT>"My Drive/"</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="FILE_ID"  FIELD="TEXT"  NUM_ROW="55" SHOW_IF="BY_ID == 'true'>
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="STORE_TO_LOCAL"  FIELD="CHECK"  NUM_ROW="56" REQUIRED="true">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="OUTPUT"  FIELD="DIRECTORY"  NUM_ROW="58" REQUIRED="true" SHOW_IF=" STORE_TO_LOCAL == 'true'">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="DIE_ON_ERROR" FIELD="CHECK" NUM_ROW="60">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>
	
    <PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="82" SHOW_IF="STORE_TO_LOCAL == 'false'">
		<TABLE READONLY="true"  IF="(STORE_TO_LOCAL=='false')">
			<COLUMN NAME="fileName" TYPE="id_String" />
			<COLUMN NAME="content" TYPE="id_Object" />
		</TABLE>
	</PARAMETER>
    
   </PARAMETERS>
   
	<ADVANCED_PARAMETERS>
	</ADVANCED_PARAMETERS>
	
   <CODEGENERATION>
      	<IMPORTS>
		</IMPORTS>
   </CODEGENERATION>

  <RETURNS>
  	 <RETURN NAME="OUTPUT_PATH" TYPE="id_String" AVAILABILITY="FLOW" SHOW_IF="STORE_TO_LOCAL == 'false'"/>
  	 <RETURN NAME="INPUT_STREAM" TYPE="java.io.InputStream" AVAILABILITY="FLOW" SHOW_IF="STORE_TO_LOCAL == 'false'"/>
  </RETURNS>
</COMPONENT>