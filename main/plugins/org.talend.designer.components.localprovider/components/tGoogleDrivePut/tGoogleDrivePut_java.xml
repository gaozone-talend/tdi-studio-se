<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20140417A"
    STARTABLE="true"
    
    SCHEMA_AUTO_PROPAGATE="false"
    DATA_AUTO_PROPAGATE="false"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Cloud/Google Drive</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1" NOT_SHOW_IF="EXPOSE_OUTPUT_STREAM=='true'"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1" NOT_SHOW_IF="EXPOSE_OUTPUT_STREAM=='false'"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" MAX_OUTPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" MAX_OUTPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK" MAX_INPUT="1" MAX_OUTPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_ERROR" MAX_INPUT="1" MAX_OUTPUT="1" />
    <CONNECTOR CTYPE="RUN_IF" MAX_INPUT="1" MAX_OUTPUT="1"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER NAME="CONNECTION" FIELD="COMPONENT_LIST"  FILTER="tGoogleDriveConnection" NUM_ROW="10"  DYNAMIC_SETTINGS="true"/>

    <PARAMETER NAME="FILE_PATH"  FIELD="FILE"  NUM_ROW="52">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="PARENT_ID"  FIELD="TEXT"  NUM_ROW="54" REQUIRED="true">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="DIE_ON_ERROR" FIELD="CHECK" NUM_ROW="60">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>
	
    <PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="82">
		<TABLE READONLY="true">
			<COLUMN NAME="fileName" TYPE="id_String" />
			<COLUMN NAME="content" TYPE="id_Object" />
		</TABLE>
	</PARAMETER>
   </PARAMETERS>
   
	<ADVANCED_PARAMETERS>
	</ADVANCED_PARAMETERS>
	
   <CODEGENERATION>
      	<IMPORTS>
		</IMPORTS>
   </CODEGENERATION>

  <RETURNS>
  	 <RETURN NAME="OUTPUT_PATH" TYPE="id_String" AVAILABILITY="FLOW"/>
  	 <RETURN NAME="FILE_NAME" TYPE="id_String" AVAILABILITY="FLOW"/>
  	 <RETURN NAME="FILE_ID" TYPE="id_String" AVAILABILITY="FLOW"/>
  </RETURNS>
</COMPONENT>